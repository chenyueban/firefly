{
  "name": "firefly",
  "type": "module",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "postinstall": "nuxi prepare",
    "typecheck": "vue-tsc --noEmit",
    "prebuild": "rimraf dist",
    "build": "nuxi build",
    "dev": "nuxi dev --port 5173",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev --port 5173",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "generate": "nuxi generate",
    "dev:desktop": "tauri dev",
    "build:desktop": "tauri build",
    "build:desktop:debug": "tauri build --debug",
    "tauri": "tauri"
  },
  "dependencies": {
    "@firefly/common": "workspace:*",
    "@firefly/editor": "workspace:*",
    "@firefly/theme": "workspace:*",
    "@tauri-apps/api": "^1.2.0",
    "@upstash/redis": "^1.20.4",
    "@vercel/analytics": "^1.0.0",
    "@yeger/vue-masonry-wall": "^3.4.4",
    "cheerio": "1.0.0-rc.12",
    "crypto-js": "^4.1.1",
    "eventsource-parser": "^1.0.0",
    "gpt-3-encoder": "^1.1.4",
    "js-base64": "^3.7.5",
    "rss-parser": "^3.13.0",
    "tauri-plugin-autostart-api": "https://github.com/tauri-apps/tauri-plugin-autostart",
    "url-metadata": "^3.0.1",
    "vue-virtual-scroller": "2.0.0-beta.8"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.4.1",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/i18n": "8.0.0-beta.9",
    "@pinia/nuxt": "^0.4.8",
    "@tauri-apps/cli": "^1.2.3",
    "@types/crypto-js": "^4.1.1",
    "@types/js-base64": "^3.3.1",
    "@types/vue-virtual-scroller": "npm:@vsme/vue-virtual-scroller@^1.0.1",
    "@unocss/nuxt": "^0.51.5",
    "@vite-pwa/nuxt": "^0.0.7",
    "@vue-macros/nuxt": "^1.2.8",
    "@vueuse/nuxt": "^10.0.2",
    "nuxt": "^3.4.2",
    "nuxt-gtag": "^0.5.6"
  }
